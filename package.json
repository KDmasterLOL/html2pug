{
  "name": "html2pug",
  "version": "4.0.0",
  "type": "module",
  "description": "Converts HTML to Pug",
  "main": "dist/index.js",
  "bin": {
    "html2pug": "dist/cli.js"
  },
  "dependencies": {
    "get-stdin": "^9.0.0",
    "html-minifier": "^4.0.0",
    "jsdom": "^23.0.1",
  },
  "devDependencies": {
    "@types/html-minifier": "^4.0.5",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.5",
    "ava": "^6.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run build; ava"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/izolate/html2pug.git"
  },
  "keywords": [
    "html",
    "pug",
    "jade",
    "convert",
    "templates"
  ],
  "author": "izolate <yoshtalwar@gmail.com> (http://izolate.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/izolate/html2pug/issues"
  },
  "homepage": "https://github.com/izolate/html2pug#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  }
}